<form id="transparentRedirectForm" action="{{ $TransparentRedirectURL }}" method="POST">
	<input type="hidden" name="HashDigest" value="{{ $HashDigest }}" />
	<input type="hidden" name="MerchantID" value="{{ $MerchantID }}" />
	<input type="hidden" name="CrossReference" value="{{ $CrossReference }}" />
	<input type="hidden" name="TransactionDateTime" value="{{ $TransactionDateTime }}" />
	<input type="hidden" name="CallbackURL" value="{{ $CallbackURL }}" />
	<input type="hidden" name="PaRES" value="{{ $PaRes }}" />
	<noscript>
		You have javascript disabled, please click to be redirected:
		<input type="submit" value="Submit">
	</noscript>
</form>

<script>
	var frm = document.getElementById("transparentRedirectForm");
	frm.submit();
</script>